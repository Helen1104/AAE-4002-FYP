--------------------------------- 

Select CalDueDate, ROW_NUMBER() OVER(ORDER BY CalDueDate) from 

(SELECT distinct CAST(CalDueDate as DATE) CalDueDate FROM data) s 

WHERE CalDueDate <= 'td = date.today()' 

ORDER BY CalDueDate 

 

 

 

 

 

